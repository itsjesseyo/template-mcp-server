{
  "name": "@mcpdotdirect/create-mcp-server",
  "module": "src/index.ts",
  "type": "module",
  "version": "2.0.1",
  "description": "CLI tool to create a new MCP (Model Context Protocol) server project",
  "private": false,
  "main": "build/index.js",
  "bin": {
    "create-mcp-server": "./bin/create-mcp-server.js"
  },
  "files": [
    "bin",
    "src/",
    "build/",
    ".gitignore",
    "tsconfig.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "node build/index.js",
    "start:http": "node build/server/http-server.js",
    "build": "tsc && mkdir -p build/bin && cp bin/create-mcp-server.js build/bin/ && cp -r src build/ && cp tsconfig.json build/ && cp README.md build/ && cp .gitignore build/ 2>/dev/null || true",
    "build:http": "tsc src/server/http-server.ts --outDir build/server",
    "dev": "nodemon --watch src --ext ts,json --exec \"ts-node --esm src/index.ts\"",
    "dev:http": "nodemon --watch src --ext ts,json --exec \"ts-node --esm src/server/http-server.ts\"",
    "prepublishOnly": "npm run build && npm run build:http",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major",
    "release": "npm publish",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "changelog:latest": "conventional-changelog -p angular -r 1 > RELEASE_NOTES.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itsjesseyo/template-mcp-server.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "template",
    "server",
    "ai",
    "agent",
    "create",
    "generator",
    "starter",
    "boilerplate"
  ],
  "author": "mcpdotdirect",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itsjesseyo/template-mcp-server/issues"
  },
  "homepage": "https://github.com/itsjesseyo/template-mcp-server#readme",
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/node": "^20.17.24",
    "conventional-changelog-cli": "^5.0.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "@valibot/to-json-schema": "^1.0.0",
    "effect": "^3.14.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "fastmcp": "^1.21.0",
    "zod": "^3.24.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
